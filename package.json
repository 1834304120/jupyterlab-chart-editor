{
  "name": "jupyterlab-chart-editor",
  "version": "0.1.0",
  "description": "A JupyterLab extension for creating and editing Plotly charts",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension",
    "plotly",
    "react-plotly.js",
    "react"
  ],
  "homepage": "https://github.com/plotly/jupyterlab-chart-editor",
  "bugs": {
    "url": "https://github.com/plotly/jupyterlab-chart-editor/issues"
  },
  "license": "BSD-3-Clause",
  "author": "Plotly, Inc.",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/plotly/jupyterlab-chart-editor.git"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib",
    "prepack": "npm run clean && npm run build",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@jupyterlab/rendermime-interfaces": "^1.0.3",
    "@phosphor/widgets": "^1.5.0",
    "plotly-icons": "gnestor/plotly-icons#package.json",
    "plotly.js": "^1.32.0",
    "react": "~16.0.0",
    "react-dom": "~16.0.0",
    "react-plotly.js": "^1.2.0",
    "react-plotly.js-editor": "gnestor/react-plotly.js-editor#package.json"
  },
  "devDependencies": {
    "@types/plotly.js": "^1.28.3",
    "@types/react": "16.0.38",
    "@types/react-dom": "16.0.4",
    "rimraf": "~2.6.2",
    "typescript": "~2.6.2"
  },
  "jupyterlab": {
    "extension": true
  }
}
